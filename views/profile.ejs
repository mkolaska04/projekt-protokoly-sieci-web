<%- include("partials/navbar.ejs") %>
    <div class="main">
        <div class="profile">
            <h1>Profile of <%= username %>
            </h1>

            <div class="posts">
                <% if (posts && posts.length> 0) { %>
                  <% posts.forEach(function(post) { %>
                    <%- include("partials/post.ejs", {post: post}) %>
                    <% }) %>
                      <% } else { %>
                        <p>No posts yet</p>
                        <% } %>
              </div>
        </div>
    </div>

    <script src="/script.js"></script>
    <%- include("partials/footer.ejs") %>